<!DOCTYPE html>
<!--[if lte IE 9]><html class="no-js legacy" {{ site.language_attributes }}><![endif]-->
<!--[if gt IE 9]><!--><html class="no-js" {{ site.language_attributes }}><!--<![endif]-->
<head>
    <meta charset="{{ site.charset }}" />

    <title>{{ site.name }} - {% if wp_title %}{{ wp_title }}{% else %}Home{% endif %}</title>

    <meta http-equiv="Accept-CH" content="DPR, Width, Viewport-Width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    
    {# <link rel="shortcut icon" href="{{theme.link}}/assets/dev/img/favicon.png"> #}
    {% include 'partials/meta-favicons.twig' %}
    {% include "partials/share.twig" %}
    {% include 'partials/gtm.twig' %} 

    <link href="https://fonts.googleapis.com/css?family=Poppins|Roboto&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="{{theme.link}}/assets/dev/css/italoexpress-main.css">

    {{ wp_head }}
</head>

<body class="{{ body_class }}" itemscope itemtype="//schema.org/WebPage">

    {% include 'partials/gtm_noscript.twig' %}

    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    {% include 'partials/header.twig' %}

    <main>

        {% block content %}{% endblock %}

    </main>


    {% include 'partials/footer.twig' %}

    {{ wp_footer }}
    
    {% block scripts %}
        <script type="text/javascript">
            var templateUrl = '{{theme.link}}';
        </script>
        
        <script type="application/javascript" src="https://maps.googleapis.com/maps/api/js?key={{ gmapi }}"></script>
        <script type="module" src="https://unpkg.com/ionicons@4.5.10-0/dist/ionicons/ionicons.esm.js"></script>
        <script nomodule="" src="https://unpkg.com/ionicons@4.5.10-0/dist/ionicons/ionicons.js"></script>
        <script type="application/javascript" src="{{theme.link}}/assets/dev/js/italoexpress-main.js"></script>
    {% endblock %}

</body>
</html>
